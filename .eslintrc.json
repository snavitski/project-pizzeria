{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": ["eslint:recommended", "prettier"],
	"parserOptions": {
		"ecmaVersion": 2015,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": ["off"],
		"quotes": ["error", "double", { "allowTemplateLiterals": true }],
		"semi": ["error", "always"],
		"no-console": ["off"]
	},
	"globals": {
		"Handlebars": false,
		"app": true,
		"dataSource": true
	}
}
